<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- Mapperとxmlのマッピング -->
<mapper namespace="in.sakamoto.sb_practiceApp.repository.UserMapper">

    <!-- ユーザー1件登録 -->
    <insert id="insertOne">
        insert into m_user(
        user_id
        ,password
        ,user_name
        ,birthday
        ,age
        ,gender
        ,department_id
        ,role
        )
        values(
        #{use_id}
        ,#{password}
        ,#{user_name}
        ,#{birthday}
        ,#{age}
        ,#{gender}
        ,#{department_id}
        ,#{role}
        )
    </insert>
</mapper>
